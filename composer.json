{
    "name": "solunes/notification",
    "description": "Paquete adicional de Notificaciones para Solunes Digital.",
    "keywords": ["laravel", "solunes", "notification"],
    "license": "MIT",
    "authors": [
        {
            "name": "Solunes Digital",
            "email": "info@solunes.com"
        }
    ],
    "require": {
        "pusher/pusher-http-laravel": "^2.3",
        "berkayk/onesignal-laravel": "0.9.*",
        "aws/aws-sdk-php-laravel": "^3.1",
        "twilio/sdk": "5.*",
        "dingo/api": "1.0.*@dev",
        "tymon/jwt-auth": "0.*",
        "solunes/master": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeds",
            "src/app",
            "src/app/Controllers",
            "src/app/Console"
       ],
        "psr-4": {
            "Solunes\\Notification\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}
